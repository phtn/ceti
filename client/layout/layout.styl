.body-layout-div
	margin-top 65px
.module-div
	margin 15px auto
	//border 1px solid rgba(0,0,0, 0.5 )
	//border-radius 5px
.module-div-i
	height 100px
	border none
	border-radius 3px
	box-shadow 3px 3px 3px rgba(0,0,0, 0.3 )
	background: #bdbdbd;
	background -moz-linear-gradient(45deg, #bdbdbd 0%, #9e9e9e 100%)
	background -webkit-linear-gradient(45deg, #bdbdbd 0%,#9e9e9e 100%)
	background linear-gradient(45deg, #bdbdbd 0%,#9e9e9e 100%)
	padding 20px
	
	// Animation
	
	-webkit-animation-duration .5s
	
.module-title
	color #ffa84c
	letter-spacing 2px
	font-weight bold
	