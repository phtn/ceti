.team-div
	height 700px
	margin-top 100px
.module-round
	margin 0 auto
	margin-top 30px
	width 100px
	height 100px
	border-radius 100px
	background: #bdbdbd;
	background -moz-linear-gradient(45deg, #bdbdbd 0%, #9e9e9e 100%)
	background -webkit-linear-gradient(45deg, #bdbdbd 0%,#9e9e9e 100%)
	background linear-gradient(45deg, #bdbdbd 0%,#9e9e9e 100%)
	box-shadow 1px 1px 1px rgba(0,0,0, 0.3 )
	padding 5px
	cursor pointer
	img
		border-radius 100px
		
#code-icon
		margin-right 15px
		color #BDBDBD
		text-shadow 1px 1px 1px rgba(0,0,0, 0.5 )
		font-size 2em
		
.heads-div
	max-width 350px

div.team-name
	h6
		letter-spacing 2px
		font-weight bold
		
div.team-title
	h6
		letter-spacing 2px
		font bolder 10px
		color #ccc
		text-shadow 1px 1px 1px rgba(0,0,0, 0.3 )
.github-head
	font-size 6.6em
.sign-with-github:hover
	color #e0e0e0
	
.module-div-ii
	margin 10px auto
	height 100px
	border none
	border-radius 3px
	box-shadow 3px 3px 3px rgba(0,0,0, 0.3 )
	background: #bdbdbd;
	background -moz-linear-gradient(45deg, #bdbdbd 0%, #9e9e9e 100%)
	background -webkit-linear-gradient(45deg, #bdbdbd 0%,#9e9e9e 100%)
	background linear-gradient(45deg, #bdbdbd 0%,#9e9e9e 100%)
	padding 10px
	max-width 300px
	
	// ANIMATION
	-webkit-animation-delay .333s
	-webkit-animation-duration .2s
	
h6.ion-quote
	text-shadow 1px 1px 1px rgba(0,0,0, 0.3 )
// ANIMATION
#jun
	-webkit-animation-delay 0s
	-webkit-animation-duration .5s
#github
	-webkit-animation-delay .1s
	-webkit-animation-duration .5s
.team-name
	-webkit-animation-delay .2s
	-webkit-animation-duration .5s
.team-title
	-webkit-animation-delay .3s
	-webkit-animation-duration .5s
h6.ion-quote
	-webkit-animation-duration .2s
	-webkit-animation-delay .444s